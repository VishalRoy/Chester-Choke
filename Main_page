        <img id="img1" /> 
        <script>
        var imgArray = new Array("0.jpeg","2.jpg","6.jpg");
        var imgCount = 0;
        function startTime() 
        {
          if(imgCount == imgArray.length) 
         {
          imgCount = 0;
         }
        document.getElementById("img1").src = imgArray[imgCount];
        imgCount++;
        setTimeout("startTime()",4000);
       }
</script>
        
       
